<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <ScrollPane prefWidth="910" prefHeight="850">
        <HBox>
            <Pane prefWidth="10"/>
            <VBox>
                <Pane prefHeight="10"/>
                <GridPane fx:id="gridPane" vgap="10" hgap="10"/>
                <Pane prefHeight="10"/>
            </VBox>
            <Pane prefWidth="10"/>
        </HBox>
    </ScrollPane>

    <Pane prefWidth="20"/>

    <VBox prefWidth="190">
        <Button fx:id="outfitCreateButton" text="New Outfit"/>
        <Pane prefHeight="10"/>
        <Button fx:id="outfitEditButton" text="Edit"/>
    </VBox>

</HBox>
