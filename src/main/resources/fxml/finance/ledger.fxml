<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <Pane prefHeight="30"/>
    <ScrollPane prefWidth="1100" prefHeight="800" styleClass="edge-to-edge">
        <HBox>
            <Pane prefWidth="30"/>
            <VBox>
                <Pane fx:id="checkPane"/>
                <Pane prefHeight="40"/>
                <Pane fx:id="replenishmentPane"/>
                <Pane prefHeight="40"/>
                <Pane fx:id="expensePane"/>
                <Pane prefHeight="40"/>
                <Pane fx:id="transferPane"/>
                <Pane prefHeight="40"/>
                <Pane fx:id="historyTablePane"/>
                <Pane prefHeight="40"/>
                <HBox>
                    <Pane fx:id="scatterPane" styleClass="black_border" minWidth="460" maxWidth="460" minHeight="450"
                          maxHeight="450"/>
                    <Pane maxWidth="100" minWidth="100"/>
                    <Pane fx:id="huddlePane" styleClass="black_border" minWidth="460" maxWidth="460" minHeight="450"
                          maxHeight="450"/>
                </HBox>
            </VBox>
        </HBox>
    </ScrollPane>

</VBox>
