<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <TableView fx:id="table" prefWidth="1000" prefHeight="630">
        <columns>
            <TableColumn prefWidth="100" text="Date" fx:id="dateColumn"/>
            <TableColumn prefWidth="300" text="From" fx:id="fromColumn"/>
            <TableColumn prefWidth="300" text="To" fx:id="toColumn"/>
            <TableColumn prefWidth="80" text="amount" fx:id="amountColumn"/>
            <TableColumn prefWidth="125" fx:id="additionalInfoColumn"/>
            <TableColumn prefWidth="70" fx:id="deleteColumn"/>
        </columns>
    </TableView>

</VBox>
