<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.*?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <VBox>
        <DatePicker fx:id="startDatePicker" maxWidth="220" minWidth="220"/>
        <Pane minHeight="15" maxHeight="15"/>
        <DatePicker fx:id="endDatePicker" maxWidth="220" minWidth="220"/>
        <Pane minHeight="15" maxHeight="15"/>
        <Button fx:id="showButton" text="Show" maxWidth="220" minWidth="220"/>
        <Pane minHeight="15" maxHeight="15"/>
        <Button fx:id="stabilityButton" text="Check Stability" maxWidth="220" minWidth="220"/>
    </VBox>

    <Pane minWidth="25" maxWidth="25"/>

    <GridPane fx:id="resultsPane" hgap="20" vgap="20" alignment="CENTER">

    </GridPane>

</HBox>
